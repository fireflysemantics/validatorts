{
  "name": "@fireflysemantics/validatorts",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ig": "npm install -g @jsdevtools/version-bump-prompt && npm install -g npm-check-updates && npm install -g npm-install-peers",
    "u": "cd projects/fs-validatorts && npm i && npm-install-peers",
    "bp": "cd projects/fs-validatorts && bump patch",
    "t": "cd projects/fs-validatorts && npx jest",
    "b": "ng build fs-validatorts",
    "p": "cp README.md ./projects/fs-validatorts && npm run bp && npm run b && cd dist/fs-validatorts/ && npm publish",
    "doc": "rm -fr doc && typedoc --entryPointStrategy expand ./projects/fs-validatorts/src/lib --out doc --exclude **/*.spec.ts",
    "sdoc": "npm run doc && http-server -o doc",
    "d": "npm run doc && node deploy.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/forms": "~12.1.2",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "@briebug/jest-schematic": "^3.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.1.2",
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@types/jasmine": "~3.8.0",
    "@types/jest": "27.0.2",
    "@types/node": "^12.11.1",
    "gh-pages": "^3.2.3",
    "jasmine-core": "~3.8.0",
    "jest": "27.2.2",
    "karma-coverage": "~2.0.3",
    "ng-packagr": "^12.1.0",
    "typedoc": "^0.22.6",
    "typescript": "~4.3.2"
  }
}
